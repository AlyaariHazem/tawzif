{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
     "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "@app/*": ["src/app/*"],
      "@core/*": ["src/app/core/*"],
      "@env/*": ["src/environments/*"]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "files": [
    "src/main.ts",
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
